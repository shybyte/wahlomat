{
  "name": "wahlomat",
  "version": "0.0.1",
  "description": "Wahlomat",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build": "npm run clean && npm run build:dirty",
    "build:dirty": "tsc -p server/tsconfig.json && npm run build:webpack",
    "clean": "rimraf public/build && rimraf tmp",
    "start:dev": "nodejs ./dev-server.js",
    "start": "pm2 start config/process.json && pm2 logs wahlomat",
    "stop": "pm2 stop wahlomat --watch",
    "watch:ts:server": "tsc -p server/tsconfig.json -w",
    "start:prod": "pm2 start config/process-prod.json",
    "stop:prod": "pm2 stop wahlomat",
    "post:update": "npm install && npm run build:dirty && pm2 restart wahlomat",
    "logs": "pm2 logs all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "typings install"
  },
  "author": "Marco Stahl",
  "license": "APGL",
  "repository": {},
  "bugs": {},
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "less": "^2.6.1",
    "less-loader": "^2.2.3",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.4.0",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "ts-loader": "0.8.2",
    "typescript": "2.0.0",
    "typings": "1.3.2",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0",
    "body-parser": "^1.15.1",
    "csv": "^1.1.0",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.1.19",
    "pm2": "^1.1.3",
    "ramda": "^0.21.0",
    "shortid": "^2.2.6"
  }
}
